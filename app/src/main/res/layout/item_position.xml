<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginBottom="@dimen/dp_16"
        android:background="@drawable/tv_cor_5dp_solid_gray"
        android:paddingStart="@dimen/dp_7"
        android:paddingTop="@dimen/dp_5"
        android:paddingEnd="@dimen/dp_7"
        android:paddingBottom="@dimen/dp_5"
        android:textColor="#4d4d4d"
        android:textSize="@dimen/sp_12"
        android:text="13453"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_companyName_str"
        app:layout_constraintTop_toBottomOf="@+id/tv_companyName_str" />

    <TextView
        android:id="@+id/tv_Titlezhihu_str"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_16"
        android:textColor="@color/colorTartextTitle"
        android:textSize="@dimen/sp_16"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_Salaryscale_str"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_22"
        android:textColor="#ed7600"
        android:textSize="@dimen/sp_14"
        app:layout_constraintBottom_toBottomOf="@+id/tv_Titlezhihu_str"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_Titlezhihu_str" />

    <TextView
        android:id="@+id/tv_companyName_str"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_12"
        android:textColor="#4d4d4d"
        android:textSize="@dimen/sp_12"
        android:text="13453"
        android:maxLines="1"
        android:ellipsize="end"
        app:layout_constraintStart_toStartOf="@+id/tv_Titlezhihu_str"
        app:layout_constraintTop_toBottomOf="@+id/tv_Titlezhihu_str" />

    <TextView
        android:id="@+id/tv_Education_str"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_8"
        android:background="@drawable/tv_cor_5dp_solid_gray"
        android:paddingStart="@dimen/dp_7"
        android:paddingTop="@dimen/dp_5"
        android:paddingEnd="@dimen/dp_5"
        android:paddingBottom="@dimen/dp_5"
        android:gravity="center"
        android:textColor="#4d4d4d"
        android:text="13453"
        android:textSize="@dimen/sp_12"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#f7f7f7"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>