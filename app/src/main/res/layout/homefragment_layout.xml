                                android:paddingTop="@dimen/dp_4"
                                android:paddingBottom="@dimen/dp_4"
                                android:background="@drawable/home_zhoupu"
                                />


                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/collection"
                            android:layout_below="@+id/workshops"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dp_97"
                            android:layout_toRightOf="@+id/view_workshop"
                            android:layout_marginLeft="@dimen/dp_9"
                            android:layout_marginTop="@dimen/dp_13"
                            >
                            <com.example.jiyin.common.widget.MLImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:src="@mipmap/zaowuji"
                                android:scaleType="fitXY"
                                app:ml_radius="@dimen/dp_10"
                                android:id="@+id/ml_Creationcollection_btn"

                                />
                            <TextView
                                android:id="@+id/bb1"
                                android:layout_marginTop="@dimen/dp_18"
                                android:layout_marginLeft="@dimen/dp_9"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="造物集"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/sp_16"
                                android:gravity="left"
                                android:includeFontPadding="false"

                                />
                            <TextView
                                android:id="@+id/bb2"
                                android:layout_below="@+id/bb1"
                                android:layout_marginTop="@dimen/dp_8"
                                android:layout_alignLeft="@+id/bb1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
