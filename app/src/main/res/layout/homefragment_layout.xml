                                android:background="@drawable/home_zhoupu"
                                />

                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/workshops"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dp_97"
                            android:layout_toRightOf="@+id/view_workshop"
                            android:layout_marginLeft="@dimen/dp_9"
                            >
                            <com.example.jiyin.common.widget.MLImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:src="@mipmap/yanxishe"
                                android:scaleType="fitXY"
